n = int(input("Enter farst number :"))
z = input("Enter the these are operetor -,+,*,/,%")
m = int(input("Enter secound number :")) 

def marks():
    if(z == "+"):
        print(n+m)
    elif(z=="-"):
        print(n-m)
    elif(z == "*"):
        print(n*m):
    elif(z == "/"):
        print(n/m)
    elif(Z == "%"):
        print(n%m)
